<xsd:schema xmlns:rsm="http://www.strom.ch" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:ccts="urn:un:unece:uncefact:documentation:standard:CoreComponentsTechnicalSpecification:2" xmlns:ram="urn:un:unece:uncefact:data:draft:ReusableAggregateBusinessInformationEntity:2" targetNamespace="http://www.strom.ch" elementFormDefault="qualified" attributeFormDefault="unqualified" version="10">
	<!--=======================================================================-->
	<!--===========================   CancellationRequestToMPA_10 Schema Module   ===================-->
	<!--=======================================================================-->
	<xsd:import namespace="urn:un:unece:uncefact:data:draft:ReusableAggregateBusinessInformationEntity:2" schemaLocation="ReusableAggregateBusinessInformationEntity_2p0.xsd"/>
	<xsd:include schemaLocation="CancellationRequestToMPA_CoreComponents_1p0.xsd"/>
	<xsd:element name="CancellationRequestToMPA_10" type="rsm:CancellationRequestToMPAType_10">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Request to Metering Point Administrator for cancellation of a process</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:complexType name="CancellationRequestToMPAType_10">
		<xsd:annotation>
			<xsd:documentation>Complex type used for defining the element Request for cancellation of a process to MPA, version 1.0</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="CancellationRequestToMPA_HeaderInformation" type="rsm:EnergyStandardBusinessDocumentHeaderType"/>
			<xsd:element name="Cancellation" maxOccurs="unbounded">
				<xsd:complexType>
					<xsd:complexContent>
						<xsd:extension base="rsm:EnergyTransactionType">
							<xsd:sequence>
								<xsd:element name="BusinessProcessID" type="rsm:ProcessIdentifierType">
									<xsd:annotation>
										<xsd:documentation xml:lang="en">Reference to the process that is to be cancelled. The ID of this process has been issued by the MPA in the response to the switch request.</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
							</xsd:sequence>
						</xsd:extension>
					</xsd:complexContent>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
</xsd:schema>
