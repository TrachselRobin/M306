<xsd:schema xmlns:clmETSO000x1="urn:un:unece:uncefact:codelist:draft:ETSO:ProcessTypeCode:1" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:ccts="urn:un:unece:uncefact:documentation:standard:CoreComponentsTechnicalSpecification:2" targetNamespace="urn:un:unece:uncefact:codelist:draft:ETSO:ProcessTypeCode:1" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<!--Root element code list-->
	<xsd:element name="ProcessTypeCode" type="clmETSO000x1:ProcessTypeCodeContentType">
		<xsd:annotation>
			<xsd:documentation xml:lang="EN">Root element for ProcessTypeCode code list</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<!--=========================================================================-->
	<!--Content type for code list-->
	<xsd:simpleType name="ProcessTypeCodeContentType">
		<xsd:annotation>
			<xsd:documentation>Content type for ProcessTypeCode code list</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:token">
			<xsd:length value="3"/>
			<xsd:enumeration value="A01">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<ccts:Name>Day ahead</ccts:Name>
						<ccts:Description>to be specified by ETSO</ccts:Description>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="A02">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<ccts:Name>Intra day on the day</ccts:Name>
						<ccts:Description>to be agreed and specified by ETSO</ccts:Description>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
