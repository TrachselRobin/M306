<xsd:schema xmlns:clmebIX00010="urn:un:unece:uncefact:codelist:draft:ebIX:DocumentTypeCode:1" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:ccts="urn:un:unece:uncefact:documentation:standard:CoreComponentsTechnicalSpecification:2" targetNamespace="urn:un:unece:uncefact:codelist:draft:ebIX:DocumentTypeCode:1" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<!--Root element code list-->
	<xsd:element name="DocumentTypeCode" type="clmebIX00010:DocumentTypeCodeContentType">
		<xsd:annotation>
			<xsd:documentation xml:lang="EN">Root element for DocumentTypeCode code list</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<!--=========================================================================-->
	<!--Content type for code list-->
	<xsd:simpleType name="DocumentTypeCodeContentType">
		<xsd:annotation>
			<xsd:documentation>Content type for DocumentTypeCode code list</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:token">
			<xsd:length value="3"/>
			<xsd:enumeration value="E01">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<ccts:Name>Start of supply</ccts:Name>
						<ccts:Description>to be specified by ebIX</ccts:Description>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="E02">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<ccts:Name>Cancellation of supply</ccts:Name>
						<ccts:Description>to be agreed and specified by ebIX</ccts:Description>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="E07">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<ccts:Name>Master data Metering Point</ccts:Name>
						<ccts:Description>to be specified by ebIX</ccts:Description>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="E08">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<ccts:Name>Master data Register (in Meter)</ccts:Name>
						<ccts:Description>to be specified by ebIX</ccts:Description>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="E31">
				<xsd:annotation>
					<xsd:documentation xml:lang="EN">
						<ccts:Name>Aggregated Metered Data</ccts:Name>
						<ccts:Description>to be specified by ebIX</ccts:Description>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="E44">
				<xsd:annotation>
					<xsd:documentation xml:lang="EN">
						<ccts:Name>Notification of change of role connected to Metering Point</ccts:Name>
						<ccts:Description>to be specified by ebIX</ccts:Description>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="E66">
				<xsd:annotation>
					<xsd:documentation xml:lang="EN">
						<ccts:Name>Validated Metered Data</ccts:Name>
						<ccts:Description>to be specified by ebIX</ccts:Description>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="E67">
				<xsd:annotation>
					<xsd:documentation xml:lang="EN">
						<ccts:Name>Request for cancellation of a business process</ccts:Name>
						<ccts:Description>to be specified by ebIX</ccts:Description>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="E68">
				<xsd:annotation>
					<xsd:documentation xml:lang="EN">
						<ccts:Name>Confirmation/Rejection of the cancellation of a business process</ccts:Name>
						<ccts:Description>to be specified by ebIX</ccts:Description>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<!--=======================================================================-->
			<!--Temporary code: to be checked and updated soon, 2008-08-21-->
		</xsd:restriction>
	</xsd:simpleType>
