<xsd:schema xmlns:clmVSE00001="urn:un:unece:uncefact:codelist:draft:VSE:DocumentTypeCode:1" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:ccts="urn:un:unece:uncefact:documentation:standard:CoreComponentsTechnicalSpecification:2" targetNamespace="urn:un:unece:uncefact:codelist:draft:VSE:DocumentTypeCode:1" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<!--Root element code list-->
	<xsd:element name="DocumentTypeCode" type="clmVSE00001:DocumentTypeCodeContentType">
		<xsd:annotation>
			<xsd:documentation xml:lang="EN">Root element for DocumentTypeCode code list</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<!--=========================================================================-->
	<!--Content type for code list-->
	<xsd:simpleType name="DocumentTypeCodeContentType">
		<xsd:annotation>
			<xsd:documentation>Content type for DocumentTypeCode code list</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:token">
			<xsd:length value="3"/>
			<xsd:enumeration value="C01">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<ccts:Name>Notification from Imbalance Settlement Responsible</ccts:Name>
						<ccts:Description>to be agreed and specified by VSE</ccts:Description>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="C02">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<ccts:Name>Notification from the Metered Data Aggregator</ccts:Name>
						<ccts:Description>to be agreed and specified by VSE</ccts:Description>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<!--=============================================================-->
			<!--=============================================================-->
		</xsd:restriction>
