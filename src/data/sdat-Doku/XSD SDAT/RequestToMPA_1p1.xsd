<xsd:schema xmlns:rsm="http://www.strom.ch" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:ccts="urn:un:unece:uncefact:documentation:standard:CoreComponentsTechnicalSpecification:2" xmlns:ram="urn:un:unece:uncefact:data:draft:ReusableAggregateBusinessInformationEntity:2" targetNamespace="http://www.strom.ch" elementFormDefault="qualified" attributeFormDefault="unqualified" version="11">
	<!--=======================================================================-->
	<!--===========================   RequestToMPA_10 Schema Module   ===================-->
	<!--=======================================================================-->
	<xsd:import namespace="urn:un:unece:uncefact:data:draft:ReusableAggregateBusinessInformationEntity:2" schemaLocation="ReusableAggregateBusinessInformationEntity_2p0.xsd"/>
	<xsd:include schemaLocation="RequestToMPA_CoreComponents_1p1.xsd"/>
	<xsd:element name="RequestToMPA_11" type="rsm:RequestToMPAType_11">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Request to Metering Point Administrator</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:complexType name="RequestToMPAType_11">
		<xsd:annotation>
			<xsd:documentation>Complex type used for defining the element Request to MPA, version 1.0</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="RequestToMPA_HeaderInformation" type="rsm:EnergyStandardBusinessDocumentHeaderType"/>
			<xsd:element name="Switch" maxOccurs="unbounded">
				<xsd:complexType>
					<xsd:complexContent>
						<xsd:extension base="rsm:EnergyTransactionType">
							<xsd:sequence>
								<xsd:element name="MeteringPoint">
									<xsd:complexType>
										<xsd:complexContent>
											<xsd:extension base="rsm:EnergyMeteringPointLocationType">
												<xsd:sequence>
													<xsd:element name="BalanceResponsible" type="rsm:EnergyPartyType" minOccurs="0" maxOccurs="1"/>
													<xsd:element name="TransportCapacityResponsible" type="rsm:EnergyPartyType" minOccurs="0" maxOccurs="1"/>
													<xsd:element name="BalanceSupplier" type="rsm:EnergyPartyType" minOccurs="0" maxOccurs="1"/>
													<xsd:element name="AncillaryServiceProvider" type="rsm:EnergyPartyType" minOccurs="0"/>
													<xsd:element name="Consumer" type="rsm:ConsumerEnergyPartyType" minOccurs="0" maxOccurs="1"/>
													<xsd:element name="SwitchDate" type="rsm:SwitchDatePeriodType"/>
													<xsd:element name="GridBillingMethod" type="rsm:GridBillingMethodType" minOccurs="0" maxOccurs="1"/>
												</xsd:sequence>
											</xsd:extension>
										</xsd:complexContent>
									</xsd:complexType>
								</xsd:element>
								<xsd:element name="ReferenceToOriginalDocumentID" type="rsm:DocumentIdentifierType" minOccurs="0">
									<xsd:annotation>
										<xsd:documentation xml:lang="en">Reference to the document that is to be cancelled, therefore only used if function code = 1</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
							</xsd:sequence>
						</xsd:extension>
					</xsd:complexContent>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
</xsd:schema>
