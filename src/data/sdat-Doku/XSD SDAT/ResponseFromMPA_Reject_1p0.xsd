<xsd:schema xmlns:rsm="http://www.strom.ch" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:ccts="urn:un:unece:uncefact:documentation:standard:CoreComponentsTechnicalSpecification:2" xmlns:ram="urn:un:unece:uncefact:data:draft:ReusableAggregateBusinessInformationEntity:2" targetNamespace="http://www.strom.ch" elementFormDefault="qualified" attributeFormDefault="unqualified" version="10">
	<!--=======================================================================-->
	<!--===========================   ResponseFromMPA_Reject_10 Schema Module   ===================-->
	<!--=======================================================================-->
	<xsd:import namespace="urn:un:unece:uncefact:data:draft:ReusableAggregateBusinessInformationEntity:2" schemaLocation="ReusableAggregateBusinessInformationEntity_2p0.xsd"/>
	<xsd:include schemaLocation="ResponseFromMPA_Reject_CoreComponents_1p0.xsd"/>
	<xsd:element name="ResponseFromMPA_Reject_10" type="rsm:ResponseFromMPA_RejectType_10">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Response from Metering Point Administrator to reject a request</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:complexType name="ResponseFromMPA_RejectType_10">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Complex type used for defining the element Response from MPA, reject, version 1.0</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="ResponseFromMPA_Reject_HeaderInformation" type="rsm:EnergyStandardBusinessDocumentHeaderType"/>
			<xsd:element name="Switch" maxOccurs="unbounded">
				<xsd:complexType>
					<xsd:complexContent>
						<xsd:extension base="rsm:EnergyTransactionType">
							<xsd:sequence>
								<xsd:element name="RequestedTransaction" type="rsm:AcceptanceStatusType">
									<xsd:annotation>
										<xsd:documentation xml:lang="en">Resulting status of the original request that is responded to in this document.</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
								<xsd:element name="ReferenceToRequestingDocumentID" type="rsm:DocumentIdentifierType">
									<xsd:annotation>
										<xsd:documentation xml:lang="en">Reference to the ID of the request that triggered this response.</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
								<xsd:element name="BusinessProcessID" type="rsm:ProcessIdentifierType">
									<xsd:annotation>
										<xsd:documentation xml:lang="en">Reference to the ID of the (Overall) Business Process to which the exchange of documents belongs.</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
								<xsd:element name="ReferenceToOriginalDocumentID" type="rsm:DocumentIdentifierType" minOccurs="0">
									<xsd:annotation>
										<xsd:documentation xml:lang="en">Reference to the document that is to be cancelled, therefore only used if function code = 1</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
							</xsd:sequence>
						</xsd:extension>
					</xsd:complexContent>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
</xsd:schema>
