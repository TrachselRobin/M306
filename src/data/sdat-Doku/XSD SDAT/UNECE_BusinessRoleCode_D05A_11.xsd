	<!-- ====================================================================== -->
	<!-- =====  Element Declarations				      ===== -->
	<!-- ====================================================================== -->
	<!-- =====  Root Element Declaration				      ===== -->
	<!-- ====================================================================== -->
	<xsd:element name="BusinessRoleCode" type="clm63035:BusinessRoleCodeContentType"/>
	<!-- ====================================================================== -->
	<!-- =====  Type Definitions					      ===== -->
	<!-- ====================================================================== -->
	<!-- =====  Type Definition: Agency Identification Code Content	      ===== -->
	<!-- ====================================================================== -->
	<xsd:simpleType name="BusinessRoleCodeContentType">
		<xsd:restriction base="xsd:token">
			<xsd:minLength value="1"/>
			<xsd:maxLength value="3"/>
			<xsd:enumeration value="EZ">
				<xsd:annotation>
					<xsd:documentation xml:lang="EN">
						<ccts:Name>TSO (Transmission System Operator)</ccts:Name>
						<ccts:Definition>yet to defined</ccts:Definition>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="DDK">
				<xsd:annotation>
					<xsd:documentation xml:lang="EN">
						<ccts:Name>Balance Responsible Party</ccts:Name>
						<ccts:Definition>yet to defined</ccts:Definition>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="MDR">
				<xsd:annotation>
					<xsd:documentation xml:lang="EN">
						<ccts:Name>Metered Data Responsiblel</ccts:Name>
						<ccts:Definition>yet to defined</ccts:Definition>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="DDX">
				<xsd:annotation>
					<xsd:documentation xml:lang="EN">
						<ccts:Name>Imbalance Settlement Responsiblel</ccts:Name>
						<ccts:Definition>yet to defined</ccts:Definition>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="DDQ">
				<xsd:annotation>
					<xsd:documentation xml:lang="EN">
						<ccts:Name>Balance Supplier</ccts:Name>
						<ccts:Definition>yet to defined</ccts:Definition>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="DDZ">
				<xsd:annotation>
					<xsd:documentation xml:lang="EN">
						<ccts:Name>Metering Point Administrator</ccts:Name>
						<ccts:Definition>yet to defined</ccts:Definition>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="DEA">
				<xsd:annotation>
					<xsd:documentation xml:lang="EN">
						<ccts:Name>Metered Data Aggregator, local</ccts:Name>
						<ccts:Definition>yet to defined</ccts:Definition>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="DEC">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<ccts:Name>Party Connected to the Grid</ccts:Name>
						<ccts:Description>to be specified by ebIX</ccts:Description>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="MAD">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<ccts:Name>Meter Administrator</ccts:Name>
						<ccts:Description>to be specified by ebIX</ccts:Description>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="PQ">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">
						<ccts:Name>Certifying Party</ccts:Name>
						<ccts:Description>A party which certifies something</ccts:Description>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
