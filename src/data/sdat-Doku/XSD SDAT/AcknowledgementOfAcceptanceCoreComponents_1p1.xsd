				<xsd:attribute name="schemeDataURI" type="xsd:anyURI" use="prohibited"/>
				<xsd:attribute name="schemeURI" type="xsd:anyURI" use="prohibited"/>
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<!--======================================================================-->
	<!--==========QDT ===    EnergyEICMeteringPoint_  Identifier. Type   ==============-->
	<!--======================================================================-->
	<xsd:complexType name="EnergyEICMeteringPointIDType">
