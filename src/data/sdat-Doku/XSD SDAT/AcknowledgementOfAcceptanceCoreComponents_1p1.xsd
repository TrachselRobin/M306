				<xsd:attribute name="schemeID" type="xsd:token" use="prohibited"/>
				<xsd:attribute name="schemeName" type="xsd:string" use="prohibited"/>
				<xsd:attribute name="schemeAgencyID" type="clm63055:AgencyIdentificationCodeContentType" use="required" fixed="305"/>
				<xsd:attribute name="schemeAgencyName" type="xsd:string" use="prohibited"/>
				<xsd:attribute name="schemeVersionID" type="xsd:token" use="prohibited"/>
				<xsd:attribute name="schemeDataURI" type="xsd:anyURI" use="prohibited"/>
				<xsd:attribute name="schemeURI" type="xsd:anyURI" use="prohibited"/>
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<!--======================================================================-->
	<!--==========QDT ===   QuantityQuality_ Code. Type   =================-->
	<!--======================================================================-->
	<xsd:complexType name="QuantityQualityCodeType">
		<xsd:simpleContent>
			<xsd:extension base="clmebIX00003:EnergyQuantityQualityCodeContentType">
				<xsd:attribute name="listID" type="clmebIX00001:SchemeIdentificationCodeContentType" use="prohibited"/>
				<xsd:attribute name="listAgencyID" type="clm63055:AgencyIdentificationCodeContentType" use="prohibited"/>
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>
	<!--======================================================================-->
	<!--==========QDT ===   EnergyProduct_ Code. Type   =================-->
	<!--=======================================================================-->
	<xsd:complexType name="ProductIDType">
		<xsd:simpleContent>
			<xsd:extension base="clmebIX00002:EnergyProductIdentificationCodeContentType">
				<xsd:attribute name="schemeAgencyID" type="clm63055:AgencyIdentificationCodeContentType" use="required" fixed="9"/>
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>
	<!--======================================================================-->
	<!--==========QDT ===   MeasurementUnit_ Code. Type   =================-->
	<!--=======================================================================-->
	<xsd:simpleType name="MeasurementUnitCodeType">
		<xsd:restriction base="clm6Recommendation20:MeasurementUnitCommonCodeContentType"/>
	</xsd:simpleType>
	<!--=======================================================================-->
	<!--==========QDT ===   BusinessReason_ Code. Type   =================-->
	<!--======================================================================-->
	<xsd:complexType name="BusinessReasonCodeType">
		<xsd:choice>
			<xsd:element name="ebIXCode" type="clmebIX00006:BusinessReasonCodeContentType" minOccurs="0"/>
			<xsd:element name="VSENationalCode" type="clmVSE00006:BusinessReasonCodeContentType" minOccurs="0"/>
		</xsd:choice>
		<xsd:attribute name="codeListAgency" type="clm63055:AgencyIdentificationCodeContentType" use="required" fixed="260"/>
		<xsd:attribute name="codeListID" type="clmebIX00001:SchemeIdentificationCodeContentType" use="optional" fixed="VSE"/>
	</xsd:complexType>
	<!--=======================================================================-->
	<!--==========QDT ===   BusinessDocumentAcceptanceReason_ Code. Type   ========-->
	<!--======================================================================-->
	<xsd:complexType name="DocumentAcceptanceReasonCodeType">
		<xsd:simpleContent>
			<xsd:extension base="clmebIX00005:DocumentAcceptanceReasonCodeContentType"/>
		</xsd:simpleContent>
	</xsd:complexType>
	<!--======================================================================-->
	<!--==========QDT ===   BusinessDocumentAcceptanceStatus_ Code. Type   ========-->
	<!--======================================================================-->
	<xsd:complexType name="DocumentAcceptanceStatusCodeType">
		<xsd:simpleContent>
			<xsd:extension base="clmebIX00004:DocumentAcceptanceStatusCodeContentType"/>
		</xsd:simpleContent>
	</xsd:complexType>
	<!--=======================================================================-->
	<!--==========QDT ===   BusinessDomain_ Code. Type   =================-->
	<!--======================================================================-->
	<xsd:complexType name="BusinessDomainCodeType">
		<xsd:simpleContent>
			<xsd:extension base="clmebIX00007:BusinessDomainCodeContentType">
				<xsd:attribute name="listAgencyID" type="clm63055:AgencyIdentificationCodeContentType" use="required" fixed="260"/>
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>
	<!--=======================================================================-->
	<!--==========QDT ===   BusinessProcess_ Code. Type   =================-->
	<!--======================================================================-->
	<xsd:complexType name="BusinessProcessCodeType">
		<xsd:simpleContent>
			<xsd:extension base="clmETSO000x1:ProcessTypeCodeContentType">
				<xsd:attribute name="listAgencyID" type="clm63055:AgencyIdentificationCodeContentType" use="required" fixed="305"/>
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>
	<!--=======================================================================-->
	<!--==========QDT ===   BusinessType_ Code. Type   =================-->
	<!--======================================================================-->
	<xsd:complexType name="BusinessTypeCodeType">
		<xsd:simpleContent>
			<xsd:extension base="clmETSO000x2:BusinessTypeCodeContentType">
				<xsd:attribute name="listAgencyID" type="clm63055:AgencyIdentificationCodeContentType" use="required" fixed="305"/>
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>
	<!--=======================================================================-->
	<!--==========QDT ===   BusinessSector_ Code. Type   =================-->
	<!--======================================================================-->
	<xsd:simpleType name="BusinessSectorCodeType">
		<xsd:restriction base="clmebIX00008:BusinessSectorCodeContentType"/>
	</xsd:simpleType>
	<!--=======================================================================-->
	<!--==========QDT ===   BusinessRole_ Code. Type   =================-->
	<!--======================================================================-->
	<xsd:simpleType name="BusinessRoleCodeType">
		<xsd:union memberTypes="clm63035:BusinessRoleCodeContentType clmebIX00020:TemporaryBusinessRoleCodeContentType"/>
	</xsd:simpleType>
	<!--=======================================================================-->
	<!--==========QDT ===   DocumentFunction_ Code. Type   =================-->
	<!--======================================================================-->
	<xsd:simpleType name="DocumentFunctionCodeType">
		<xsd:restriction base="clmebIX00009:DocumentFunctionCodeContentType"/>
	</xsd:simpleType>
	<!--=======================================================================-->
	<!--==========QDT ===   DocumentType_ Code. Type   =================-->
	<!--======================================================================-->
	<xsd:complexType name="DocumentTypeCodeType">
		<xsd:choice>
			<xsd:element name="ebIXCode" type="clmebIX00010:DocumentTypeCodeContentType" minOccurs="0" maxOccurs="0"/>
			<xsd:element name="VSENationalCode" type="clmVSE00001:DocumentTypeCodeContentType" minOccurs="0" maxOccurs="0"/>
			<xsd:element name="CefactCode" type="clm61001:DocumentTypeCodeContentType" fixed="312"/>
		</xsd:choice>
		<xsd:attribute name="listAgencyID" type="clm63055:AgencyIdentificationCodeContentType" use="prohibited" fixed="260"/>
		<xsd:attribute name="listID" type="clmebIX00001:SchemeIdentificationCodeContentType" use="prohibited" fixed="VSE"/>
	</xsd:complexType>
	<!--=======================================================================-->
	<!--==========QDT ===   DocumentReferenceType_ Code. Type   =================-->
	<!--======================================================================-->
	<xsd:complexType name="DocumentReferenceTypeCodeType">
		<xsd:choice>
			<xsd:element name="ebIXCode" type="clmebIX00010:DocumentTypeCodeContentType"/>
			<xsd:element name="VSENationalCode" type="clmVSE00001:DocumentTypeCodeContentType"/>
			<xsd:element name="CefactCode" type="clm61001:DocumentTypeCodeContentType"/>
		</xsd:choice>
		<xsd:attribute name="listAgencyID" type="clm63055:AgencyIdentificationCodeContentType" use="optional" fixed="260"/>
		<xsd:attribute name="listID" type="clmebIX00001:SchemeIdentificationCodeContentType" use="optional" fixed="VSE"/>
	</xsd:complexType>
	<!--======================================================================-->
	<!--==========QDT ===   MeterReadingOrigin_ Code. Type   =================-->
	<!--======================================================================-->
	<xsd:complexType name="MeterReadingOriginCodeType">
		<xsd:simpleContent>
			<xsd:extension base="clmebIX00021:MeterReadingOriginCodeContentType">
				<xsd:attribute name="listAgencyID" type="clm63055:AgencyIdentificationCodeContentType" use="required" fixed="260"/>
				<xsd:attribute name="listID" use="prohibited">
					<xsd:simpleType>
						<xsd:restriction base="clmebIX00001:SchemeIdentificationCodeContentType"/>
					</xsd:simpleType>
				</xsd:attribute>
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>
	<!--=======================================================================-->
	<!--=================    Qualified data types based on DateTime. Type   =============-->
	<!--======================================================================-->
	<!--==========QDT ===   Energy_ Date. Type   =================-->
	<!--======================================================================-->
	<xsd:simpleType name="EnergyDateType">
		<xsd:restriction base="udt:DateType">
			<xsd:pattern value=".{4}-.{2}-.{2}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!--======================================================================-->
	<!--==========QDT ===   Energy_ DateTime. Type   =================-->
	<!--======================================================================-->
	<xsd:simpleType name="EnergyDateTimeType">
		<xsd:restriction base="udt:DateTimeType">
			<xsd:pattern value=".{4}-.{2}-.{2}T.{2}:.{2}:.{2}Z"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!--======================================================================-->
	<!--=================    Qualified data types based on Text. Type   =============-->
	<!--======================================================================-->
	<!--==========QDT ===   Simple_ Text. Type   =================-->
	<xsd:complexType name="SimpleTextType">
		<xsd:simpleContent>
			<xsd:restriction base="udt:TextType">
				<xsd:maxLength value="70"/>
				<xsd:attribute name="languageCode" use="prohibited"/>
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<!--======================================================================-->
	<!--=================    Qualified data types based on Volume. Type   =============-->
	<!--======================================================================-->
	<xsd:complexType name="EnergyVolumeType">
		<xsd:simpleContent>
			<xsd:restriction base="udt:QuantityType">
				<xsd:fractionDigits value="3" fixed="true"/>
				<xsd:attribute name="unitCode" type="clm6Recommendation20:MeasurementUnitCommonCodeContentType" use="prohibited"/>
			</xsd:restriction>
		</xsd:simpleContent>
	</xsd:complexType>
	<!--======================================================================-->
	<!-- ===== Qualified Data Type based on generic XSD Type  ========================== -->
	<!-- =================================================================== -->
	<!--============   Resolution. Type   ==============================-->
	<!--======================================================================-->
	<xsd:complexType name="ResolutionType">
		<xsd:sequence>
			<xsd:element name="Resolution" type="xsd:positiveInteger" fixed="15"/>
			<xsd:element name="Unit" type="clm6Recommendation20:MeasurementUnitCommonCodeContentType" fixed="MIN"/>
		</xsd:sequence>
	</xsd:complexType>
	<!--======================================================================-->
	<!--============   SequencePosition. Type   ==============================-->
	<xsd:simpleType name="SequencePositionType">
		<xsd:restriction base="xsd:positiveInteger">
			<xsd:totalDigits value="5"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!--======================================================================-->
	<!--==========QDT ===   MeteringPointType_ Code. Type   =================-->
	<!--======================================================================-->
	<xsd:complexType name="MeteringPointTypeCodeType">
		<xsd:simpleContent>
			<xsd:extension base="clmebIX00101:MeteringPointTypeCodeContentType">
				<xsd:attribute name="listAgencyID" type="clm63055:AgencyIdentificationCodeContentType" use="required" fixed="260"/>
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>
	<!--======================================================================-->
	<!--==========QDT ===   MeteringMethod_ Code. Type   =================-->
	<!--======================================================================-->
	<xsd:complexType name="MeteringMethodCodeType">
		<xsd:simpleContent>
			<xsd:extension base="clmebIX00103:MeteringMethodCodeContentType">
				<xsd:attribute name="listAgencyID" type="clm63055:AgencyIdentificationCodeContentType" use="required" fixed="260"/>
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>
	<!--======================================================================-->
	<!--==========QDT ===   SettlementMethod_ Code. Type   =================-->
	<!--======================================================================-->
	<xsd:complexType name="SettlementMethodCodeType">
		<xsd:simpleContent>
			<xsd:extension base="clmebIX00102:SettlementMethodCodeContentType">
				<xsd:attribute name="listAgencyID" type="clm63055:AgencyIdentificationCodeContentType" use="required" fixed="260"/>
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>
	<!--======================================================================-->
	<!--==========QDT ===   MeterTimeFrame_ Code. Type   =================-->
	<!--======================================================================-->
	<xsd:complexType name="MeterTimeFrameCodeType">
		<xsd:simpleContent>
			<xsd:extension base="clmebIX00104:MeterTimeFrameCodeContentType">
				<xsd:attribute name="listAgencyID" type="clm63055:AgencyIdentificationCodeContentType" use="required" fixed="260"/>
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>
	<!--======================================================================-->
	<!--==========QDT ===   Version_ Identifier. Type   =================-->
	<!--======================================================================-->
	<xsd:complexType name="VersionIdentifierType">
		<xsd:simpleContent>
			<xsd:extension base="idsebIX00012:VersionIdentifierContentType">
				<xsd:attribute name="listAgencyID" type="clm63055:AgencyIdentificationCodeContentType" use="required" fixed="260"/>
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>
	<!--======================================================================-->
	<!--==========QDT ===   Document_ Identifier. Type   =================-->
	<!--======================================================================-->
	<xsd:simpleType name="DocumentIdentifierType">
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="35"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!--======================================================================-->
	<!--==========QDT ===   Process_ Identifier. Type   =================-->
	<!--======================================================================-->
	<xsd:simpleType name="ProcessIdentifierType">
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="35"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!--======================================================================-->
	<!--==============   Standard Business Document Header   ========================-->
	<!--======================================================================-->
	<!--=====================   SBDH ABIE's   ====================================-->
	<!--======================================================================-->
	<xsd:complexType name="EnergyStandardBusinessDocumentHeaderType">
		<xsd:sequence>
			<xsd:element name="HeaderVersion" type="rsm:SimpleTextType" fixed="1.0"/>
			<xsd:element name="Sender" type="rsm:SenderPartyType"/>
			<xsd:element name="Receiver" type="rsm:ReceiverPartyType"/>
			<xsd:element name="InstanceDocument" type="rsm:InstanceDocumentType"/>
			<xsd:element name="BusinessScopeProcess" type="rsm:BusinessScopeProcessType"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="SenderPartyType">
		<xsd:sequence>
			<xsd:element name="ID" type="rsm:EnergyPartyIDType"/>
			<xsd:element name="Role" type="rsm:BusinessRoleCodeType"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="ReceiverPartyType">
		<xsd:sequence>
			<xsd:element name="ID" type="rsm:EnergyPartyIDType"/>
			<xsd:element name="Role" type="rsm:BusinessRoleCodeType"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="InstanceDocumentType">
		<xsd:sequence>
			<xsd:element name="DictionaryAgencyID" type="clm63055:AgencyIdentificationCodeContentType" fixed="260"/>
			<xsd:element name="VersionID">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">version of the standard ebIX/VSE document schema</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:simpleContent>
						<xsd:restriction base="rsm:VersionIdentifierType">
							<xsd:pattern value="2007B"/>
							<xsd:attribute name="listAgencyID" type="clm63055:AgencyIdentificationCodeContentType" use="required" fixed="260"/>
						</xsd:restriction>
					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="DocumentID" type="rsm:DocumentIdentifierType"/>
			<xsd:element name="DocumentType" type="rsm:DocumentTypeCodeType"/>
			<xsd:element name="Creation" type="rsm:EnergyDateTimeType"/>
			<xsd:element name="Status">
				<xsd:simpleType>
					<xsd:restriction base="rsm:DocumentFunctionCodeType">
						<xsd:pattern value="9"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="BusinessScopeProcessType">
		<xsd:sequence>
			<xsd:element name="BusinessReasonType" type="rsm:BusinessReasonCodeType" minOccurs="0" maxOccurs="0"/>
			<xsd:element name="ProcessType" type="rsm:BusinessProcessCodeType" minOccurs="0" maxOccurs="0"/>
			<xsd:element name="BusinessDomainType" type="rsm:BusinessDomainCodeType"/>
			<xsd:element name="BusinessSectorType" type="rsm:BusinessSectorCodeType"/>
			<xsd:element name="ReportPeriod" type="rsm:DateTimePeriodType" minOccurs="0" maxOccurs="0">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Period beyond which no information within the business document shall be specified.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="BusinessService" type="rsm:BusinessServiceType"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="BusinessServiceType">
		<xsd:sequence>
			<xsd:element name="BusinessServiceName" type="xsd:string" minOccurs="0" maxOccurs="0"/>
			<xsd:element name="ServiceTransaction" type="rsm:ServiceTransactionType"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="ServiceTransactionType">
		<xsd:attribute name="typeOfServiceTransaction" type="rsm:TypeOfServiceTransaction" use="prohibited"/>
		<xsd:attribute name="isNonRepudiationRequired" type="xsd:string" use="prohibited"/>
		<xsd:attribute name="isAuthenticationRequired" type="xsd:string" use="prohibited"/>
		<xsd:attribute name="isNonRepudiationOfReceiptRequired" type="xsd:string" use="prohibited"/>
		<xsd:attribute name="isIntelligibleCheckRequired" type="xsd:boolean" use="required" fixed="false"/>
		<xsd:attribute name="isApplicationErrorResponseRequested" type="xsd:boolean" use="prohibited"/>
		<xsd:attribute name="timeToAcknowledgeReceipt" type="xsd:string" use="prohibited"/>
		<xsd:attribute name="timeToAcknowledgeAcceptance" type="xsd:string" use="prohibited"/>
		<xsd:attribute name="timeToPerform" type="xsd:string" use="prohibited"/>
		<xsd:attribute name="recurrence" type="xsd:positiveInteger" use="prohibited"/>
	</xsd:complexType>
	<xsd:simpleType name="TypeOfServiceTransaction">
		<xsd:restriction base="xsd:string"/>
	</xsd:simpleType>
</xsd:schema>
